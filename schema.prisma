// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource postgresql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id                     String    @id @default(cuid())
  username               String    @default("")
  email                  String    @unique @default("")
  password               String
  from_Product_createdBy Product[] @relation("Product_createdBy")
}

model Product {
  id          String        @id @default(cuid())
  name        String        @default("")
  description String        @default("")
  price       Int?
  stock       Int?
  createdBy   User?         @relation("Product_createdBy", fields: [createdById], references: [id])
  createdById String?       @map("createdBy")
  createdAt   DateTime?     @default(now())
  updatedAt   DateTime?
  status      String?       @default("DRAFT")
  photo       ProductImage? @relation("Product_photo", fields: [photoId], references: [id])
  photoId     String?       @unique @map("photo")

  @@index([createdById])
}

model ProductImage {
  id      String   @id @default(cuid())
  image   Json?
  altText String   @default("")
  product Product? @relation("Product_photo")
}